
@model List<Feature>
<!-- /content-6-->
<section class="w3l-content-6 py-5">
    <div class="container py-lg-5">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-6 content-6-left pr-lg-5">
                    <h3 class="hny-title">@item.Post1Name</h3>
                </div>
                <div class="col-lg-6 content-6-right mt-lg-0 mt-4">
                    <p class="mb-3">
                        @Html.Raw(item.Post1Description)
                    </p>
                    <a href="/Destination/Index/" class="btn btn-style btn-success mt-4">Göz At</a>
                </div>
            }
        </div>
    </div>
</section>